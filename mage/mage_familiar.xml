          <position>insidetop</position>
          <offset>0,13</offset>
        </anchored>
        <center/>
        <static>Corpus/Health</static>
        <font>sheetlabel</font>
      </StringControl>
      <DotBoxControl name="fhealth">
        <anchored>
          <top>
            <parent>fhealthlabel</parent>
            <anchor>bottom</anchor>
            <offset>1</offset>
          </top>
          <left>
            <parent>fhealthlabel</parent>
            <anchor>left</anchor>
            <offset>70</offset>
          </left>
          <size>
            <width>120</width>
            <height>25</height>
          </size>
        </anchored>
        <degree>1</degree>
        <dots>10</dots>
        <nodenames>
          <slash>spent</slash>
        </nodenames>
      </DotBoxControl>

      <StringControl name="esslabel">
        <anchored>
