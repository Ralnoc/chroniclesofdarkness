 <!-- Skills -->
      <genericcontrol name="fskllframe">
        <bounds>12,168,240,120</bounds>
        <frame>
          <name>sheetgroup</name>
        </frame>
      </genericcontrol>
<StringControl>
        <anchored>
          <to>fskllframe</to>
          <position>insidetopleft</position>
          <offset>12,13</offset>
        </anchored>
        <font>sheetlabel</font>
        <static>Skills</static>
      </StringControl>
      <windowlist name="fskills">
        <allowcreate/>
        <allowdelete/>
        <anchored>
          <top>
            <parent>fskllframe</parent>
            <anchor>top</anchor>
            <offset>24</offset>
          </top>
          <bottom>
            <parent>fskllframe</parent>
            <anchor>bottom</anchor>
            <offset>-16</offset>
          </bottom>
          <left>
            <parent>fskllframe</parent>
            <anchor>left</anchor>
            <offset>13</offset>
          </left>
          <right>
            <parent>fskllframe</parent>
            <anchor>right</anchor>
            <offset>-12</offset>
          </right>
        </anchored>
        <class>charsheet_listitem</class>
        <datasource>.fskills</datasource>
        <script>
          function onInit()
            checkEmpty();
          end
          
          function onListRearranged(changed)
            if changed then
              checkEmpty();
            end
          end
          
          function checkEmpty()
            if #getWindows()==0 then
              createWindow();
            end
          end
        </script>
      </windowlist>
      <scrollercontrol>
        <anchored>
          <to>fskills</to>
          <position>insidebottomright</position>
          <size>
            <width>45</width>
            <height>27</height>
          </size>
        </anchored>
        <target>fskills</target>
        <button>
          <normal>button_scroller</normal>
          <pressed>button_scroller_down</pressed>
        </button>
      </scrollercontrol>



      <!-- Corpus/Heath / Willpower / Essence -->
      <genericcontrol name="mgspcframe">
        <bounds>252,8,240,160</bounds>
        <frame>
          <name>sheetgroup</name>
