          </left>
          <size>
            <width>120</width>
            <height>25</height>
          </size>
        </anchored>
        <dots>10</dots>
        <stateicons>
          <on>box_cross</on>
          <off>box_empty</off>
        </stateicons>
      </DotControl>

      <StringControl name="fwillplabel">
